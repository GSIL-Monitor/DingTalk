<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <solid android:color="@color/ui_common_level3_button_pressed_bg_color" />
            <stroke android:width="1.0px" android:color="@color/ui_common_button_border_color" />
            <corners android:radius="5.0dip" />
        </shape>
    </item>
    <item>
        <shape android:shape="rectangle">
            <solid android:color="@color/uidic_global_color_transparent" />
            <corners android:radius="5.0dip" />
            <stroke android:width="1.0px" android:color="@color/ui_common_button_border_color" />
        </shape>
    </item>
</selector>